    "prop-types": "15.8.1",
    "react": "^19.2.0",
    "react-colorful": "5.6.1",
    "react-router-dom": "7.9.6",
    "tinycolor2": "1.6.0",
    "use-immer": "0.11.0"
  },
  "devDependencies": {
    "@types/js-beautify": "1.14.3",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/tinycolor2": "1.4.6",
    "@vitejs/plugin-react": "5.1.1",
    "jsdom": "27.2.0",
    "sass": "1.94.1",
    "vite": "7.2.2",
