    "tscw": "tsc --watch --skipLibCheck",
    "watch": "vite watch",
    "testOnce": "vitest run"
  },
  "dependencies": {
    "@utahdts/utah-design-system": "3.0.5",
    "@utahdts/utah-design-system-header": "3.0.5",
