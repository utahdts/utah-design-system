    "tscw": "tsc --watch --skipLibCheck",
    "watch": "vite watch",
    "testOnce": "vitest run"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@utahdts/utah-design-system": "1.13.2",
    "@utahdts/utah-design-system-header": "1.13.2",
    "js-beautify": "1.14.11",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-popper": "2.3.0",
    "react-router-dom": "^6.17.0",
    "tinycolor2": "^1.4.2",
    "use-immer": "0.9.0"
  },
  "devDependencies": {
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.17",
    "@vitejs/plugin-react": "4.2.1",
    "@types/tinycolor2": "1.4.6",
    "jsdom": "23.0.1",
    "sass": "1.69.5",
    "vite": "5.0.8",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-mkcert": "1.17.1",
    "vitest": "1.0.4"
  },
  "eslint.workingDirectories": [
    "./src",
    "./test"
  ]
