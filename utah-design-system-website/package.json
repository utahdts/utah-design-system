    "prop-types": "15.8.1",
    "react": "^19.1.0",
    "react-colorful": "5.6.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "7.4.0",
    "tinycolor2": "1.6.0",
    "use-immer": "0.11.0"
  },
  "devDependencies": {
    "@types/js-beautify": "^1.14.3",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/tinycolor2": "1.4.6",
    "@vitejs/plugin-react": "4.3.4",
    "jsdom": "26.0.0",
    "sass": "1.86.0",
    "vite": "6.2.2",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-mkcert": "1.17.8",
