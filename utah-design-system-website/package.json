{
  "name": "utah-design-system-website",
  "private": true,
  "version": "3.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development-local",
    "build": "vite build --mode production-website",
    "buildw": "vite build --watch --mode production-website",
    "eslint": "npx eslint",
    "eslintw": "npx eslint-watch -w",
    "preview": "vite preview",
    "publishLibrary": "npm publish --access public",
    "test": "vitest",
    "testw": "vitest",
    "testc": "vitest run --coverage",
    "tsc": "tsc",
    "tscw": "tsc --watch --skipLibCheck",
    "watch": "vite watch",
    "testOnce": "vitest run"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@utahdts/utah-design-system": "3.0.3",
    "@utahdts/utah-design-system-header": "3.0.3",
    "date-fns": "4.1.0",
    "js-beautify": "1.15.1",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-colorful": "5.6.1",
    "react-dom": "18.3.1",
    "react-popper": "2.3.0",
    "react-router-dom": "6.28.0",
    "tinycolor2": "1.6.0",
    "use-immer": "0.10.0"
  },
  "devDependencies": {
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/tinycolor2": "1.4.6",
    "@vitejs/plugin-react": "4.3.4",
    "jsdom": "25.0.1",
    "sass": "1.82.0",
    "vite": "6.0.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-mkcert": "1.17.6",
    "vitest": "2.1.8"
  },
  "eslint.workingDirectories": [
    "./src",
    "./test"
  ]
}
